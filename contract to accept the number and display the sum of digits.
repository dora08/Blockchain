pragma solidity ^0.5.17;
contract SumOfDigits {
    function calculateSumOfDigits(uint256 num) public pure returns (uint256) {
        uint256 sum = 0;        
        while (num != 0) {
            sum += num % 10; // Add the last digit to the sum
            num /= 10; // Remove the last digit
        }       
        return sum;
    }
}
