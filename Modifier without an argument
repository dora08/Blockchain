pragma solidity >=0.4.22 <0.7.0;

contract modifierWithoutArg {
   address admin; 
  struct employee
  
  {
    uint emp_id;
    string emp_name;
    uint age;
   }
     
   constructor() public 
   {
     admin = msg.sender;
   }
      
    modifier isAdmin
    {
        require(admin == msg.sender);
        _;
    }
  employee e;
 function enterDetails (uint _empid, string memory _empname, 
                        uint _empage) public isAdmin {
   e.emp_id = _empid;
   e.emp_name = _empname;
   e.age = _empage;
 }
}


op on rhs
